{
  "manifest_version": 3,
  "name": "Github Extension",
  "version": "1.0",
  "permissions": ["storage", "https://api.notion.com/*"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup/popup.html"
  },
  "options_ui": {
    "page": "options/options.html",
    "open_in_tab": true
  }
}
